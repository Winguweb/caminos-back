- if (tracking_id = GOOGLE_ANALYTICS['tracking_id']).present?
  = javascript_include_tag "https://www.googletagmanager.com/gtag/js?id=#{tracking_id}", async: true
  javascript:
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());
    gtag('config', #{tracking_id});
